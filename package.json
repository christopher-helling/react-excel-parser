{
  "name": "react-excel-parser",
  "version": "0.0.2",
  "description": "A react library to render and display excel sheets on webpage",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "mode": "production",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint --ext .ts --ext .tsx ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopher-helling/react-excel-parser.git"
  },
  "keywords": [
    "react",
    "react-dom",
    "excel",
    "sheetjs",
    "workbook",
    "table"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "author": "Christopher Helling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christopher-helling/react-excel-parser/issues"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-react": "^1.1.7",
    "tslib": "^2.5.0",
    "typescript": "^4.6.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^16.13.1",
    "ts-loader": "^9.4.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.1"
  }
}
